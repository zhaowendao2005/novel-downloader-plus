}

/* 通用按钮样式 - iOS 风格 */
PushButton, QPushButton {
    background-color: #007AFF; /* 系统蓝色 */
    color: white;
    border: none;
    border-radius: 8px;
    padding: 8px 16px;
    min-height: 28px;
    font-weight: 500;
}

PushButton:hover, QPushButton:hover {
    background-color: #0063CC; /* 悬停加深 */
}

PushButton:pressed, QPushButton:pressed {
    background-color: #004299; /* 按压效果 */
}

/* 输入框 - 现代扁平风格 */
LineEdit, QLineEdit, QPlainTextEdit {
    background-color: rgba(255,255,255,0.9);
    border: 1px solid #C6C6C8;
    border-radius: 8px;
    padding: 6px 12px;
    selection-background-color: #007AFF;
}

LineEdit:focus, QLineEdit:focus, QPlainTextEdit:focus {
    border-color: #007AFF;
    box-shadow: 0 0 0 2px rgba(0,122,255,0.2); /* iOS焦点效果 */
}

/* 工具盒(QToolBox) - 侧边栏风格 */ 
QToolBox::tab {
    background: rgba(242,242,247,0.8);
    border-radius: 8px;
    margin: 4px;
    padding: 12px;
}

QToolBox::tab:selected {
    background: rgba(0,122,255,0.1);
    color: #007AFF;
}

/* 堆叠窗口(QStackedWidget) - 卡片式布局 */
QStackedWidget QFrame {
    background: white;
    border-radius: 12px;
    border: 1px solid rgba(0,0,0,0.1);
    margin: 8px;
    padding: 16px;
}

/* 标签文本 - 强调层次 */
QLabel {
    font-size: 13pt;
    color: #3A3A3C;
    qproperty-alignment: AlignCenter;
}

/* 分隔间距 - iOS标准间距 */
QSpacerItem {
    width: 16px;  /* 水平间距 */
    height: 16px; /* 垂直间距 */
}

/* 滚动条 - 简约风格 */
QScrollBar:vertical {
    width: 6px;
    background: transparent;
}

QScrollBar::handle:vertical {
    background: #C6C6C8;
    border-radius: 3px;
}
